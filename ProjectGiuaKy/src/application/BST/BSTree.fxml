<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BST.Controller">
   <bottom>
      <HBox fx:id="hBox" alignment="CENTER" prefHeight="44.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER_RIGHT">
         <children>
            <Label prefHeight="17.0" prefWidth="77.0" text="Enter a key:" />
            <TextField fx:id="txt1" prefHeight="25.0" prefWidth="71.0" />
            <Button mnemonicParsing="false" onAction="#insertOnAction" text="Insert" />
            <Button mnemonicParsing="false" onAction="#deleteOnAction" text="Delete" />
            <Button mnemonicParsing="false" onAction="#searchOnAction" text="Search" />
            <Button mnemonicParsing="false" onAction="#backOnAction" text="Back" />
         </children>
         <opaqueInsets>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <AnchorPane fx:id="Apane" prefHeight="0.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
