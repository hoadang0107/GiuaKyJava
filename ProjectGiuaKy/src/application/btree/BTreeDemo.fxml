<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="444.0" prefWidth="612.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.btree.BTreeController">
   <top>
      <HBox prefHeight="57.0" prefWidth="612.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="txtfield" prefHeight="34.0" prefWidth="160.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="btnInsert" mnemonicParsing="false" onAction="#insertAction" prefHeight="34.0" prefWidth="80.0" text="Insert">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btnSearch" mnemonicParsing="false" onAction="#searchAction" prefHeight="34.0" prefWidth="80.0" text="Search">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btndel" mnemonicParsing="false" onAction="#delAction" prefHeight="34.0" prefWidth="80.0" text="Delete">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#backAction" prefHeight="34.0" prefWidth="80.0" text="Back">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
   <bottom>
      <HBox prefHeight="60.0" prefWidth="612.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="mtxtArea" prefHeight="60.0" prefWidth="611.0" />
         </children>
      </HBox>
   </bottom>
   <center>
      <Pane fx:id="mPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
