<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.linkedlist.LinkedListController">
   <top>
      <HBox prefHeight="71.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textf" prefHeight="34.0" prefWidth="194.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="btnAppend" mnemonicParsing="false" onAction="#appendAction" prefHeight="34.0" prefWidth="80.0" text="append">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnPrepend" mnemonicParsing="false" onAction="#prependAction" prefHeight="34.0" prefWidth="80.0" text="prepend">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#deleteAction" prefHeight="34.0" prefWidth="80.0" text="delete" />
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
   <bottom>
      <Button mnemonicParsing="false" onAction="#backOnAction" prefHeight="30.0" prefWidth="75.0" text="Back" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <center>
      <AnchorPane fx:id="anchor" prefHeight="337.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
         <children>
            <Label fx:id="lbHead" layoutX="248.0" layoutY="26.0" prefHeight="29.0" prefWidth="35.0" />
            <Label layoutX="212.0" layoutY="31.0" prefHeight="18.0" prefWidth="36.0" text="Head" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
